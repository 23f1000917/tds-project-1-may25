{
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "app.py",
            "methods": ["GET"] 
        },
        {
            "src": "/api",
            "dest": "app.py",
            "methods": ["POST"] 
        }
    ]
}